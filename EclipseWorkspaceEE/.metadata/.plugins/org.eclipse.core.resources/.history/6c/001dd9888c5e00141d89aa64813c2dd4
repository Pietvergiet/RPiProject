
public class RandomZooi {
	public static void main(String[] arg) {
		boolean input[] = {true, true, true, true, true, true, true, true};
		int n = 0;
		for (int i = 0; i < 8; ++i) {
		    n = (n << 1) + (input[i] ? 1 : 0);
		}
		System.out.println(n);
		
		char a = (char) 65;
		char b = (char) 66;
		char c = (char) 67;
		char d = (char) 68;
		StringBuilder string = new StringBuilder();
		string.append(a);
		string.append(a);
		string.append(d);
		string.append(c);
		string.append(b);
		System.out.println(string);
	}
}
